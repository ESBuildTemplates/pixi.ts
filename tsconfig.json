{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "lib": ["ESNext", "dom"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "outDir": "./dist"
  }
}
